import java.util.Scanner;
public class javastudy {

		public static void main(String[] args)
		{
		Scanner obj=new Scanner(System.in);
		int i,n,Sum;
		System.out.print("Enter the number:");
		n=obj.nextInt();
		Sum=0;
		for(i=1;i<=n;i++)
		{
					Sum=Sum+i;
}
		{
			System.out.print("sum is:"+Sum);
		}
}}

