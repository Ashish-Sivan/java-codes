public class javastudy {

		public static void main(String[] args)
		{
		Scanner obj=new Scanner(System.in);
		int[] a={3,6,7,8,9};
		for(int i=0;i<=4;i++)
		{
			System.out.println(a[i]);
		}
		System.out.println(a[2]);
		}}

