import java.util.Scanner;
public class javastudy {

		public static void main(String[] args)
		{
		Scanner obj=new Scanner(System.in);
		int i;
		for(i=1;i<=10;i++)
		{
			System.out.println("hello");
			System.out.println(i);
		}
}}



